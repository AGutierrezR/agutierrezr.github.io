---
import MainLayout from './MainLayout.astro'

const { frontmatter } = Astro.props
---

<MainLayout title={frontmatter.metaTitle} description={frontmatter.metaDesc}>
  <main tabindex="-1" id="main-content" class="cover">
    <div class="home-intro wrapper flow">
      <slot />
    </div>
  </main>
</MainLayout>
